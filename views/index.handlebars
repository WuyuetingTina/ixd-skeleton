<body>
		<div class="top">
			<form class="example">
				<button id="upload" type="button" onclick= "window.location.href='/upload'"><i class="fa fa-plus" style="color:rgb(153, 58, 58);"></i></button>
				<input type="text" id="myInput" onkeyup="myFunctionSearch()" placeholder="Search for items..." title="Type in a name">
				<i id="question" class="far fa-question-circle fa-2x" style="color: rgb(153, 58, 58);"></i>


				{{!-- <a href="/search"><input type="button" value="Search"></a> --}}
			</form>

			{{!-- <dialog id='dialog'></dialog> --}}
		</div>

		<div class= "post">
			{{#each projects}}
			<div class="project" id="{{id}}">
				<a href="/project/{{id}}">
				<div class='food'><img id="photo" width="120" height="120" src="{{image}}"></div>
				<div class="words"><b><p class="description" style="color: rgb(95, 93, 93);">{{name}} <br> ({{time_estimate}})</p></b></div>
				</a>
			</div>
			{{/each}}
		</div>

		<div id="buttom">
			<a class="icon1" href="/index"><i class="fa fa-home fa-3x" style="color:rgb(153, 58, 58)"></i></a>
			<a class="icon2" href="/like"><i class="fa fa-heart fa-3x" style="color:rgb(153, 58, 58)"></i></a>
			<a class="icon3" href="/Profile"><i class="fa fa-user-circle fa-3x" style="color:rgb(153, 58, 58)"></i></a>
			<br>
		</div>

</div>
</body>
</html>


